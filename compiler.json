{
    "type": "compiler",
    "version": "1.0.0",
    "short-name":"matjuice",
    "supported-languages":[
        "matlab"
    ],
    "target-languages":[
        "js"
    ],
    "runner-name":"runner.js",
    "commands": [
        {   "executable-path": { "file": "./release/matjuice-v1.0.0/matjuice" },
            "options": [
                { "config": "/implementation/runner-source-file" },
                "unbundled.js",
                { "config": "/implementation/mclab-core-runner-argument-types" }
            ]
        },
        {   "executable-path": { "file": "./browserify/bin/cmd.js" },
            "options": [
                "-f ",
                "unbundled.js",
                "-o ",
                { "config": "/compiler/runner-name" }
            ]
        }
    ],
    "dependencies": [
        {
            "source": "https://github.com/Sable/matjuice/releases/download/v1.0.0/matjuice-v1.0.0.tar.gz",
            "destination": { "file": "./release" }
        }
    ]
}
